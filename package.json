{
  "name": "express-js-fullstack-template",
  "version": "3.4.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "build": "npm run build:scss",
    "build:scss": "sass --style=compressed --error-css --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css",
    "dev": "SET DEBUG=express-js-fullstack-template:* & nodemon",
    "dev:scss:watch": "npm run build:scss && sass --style=compressed --error-css --update --watch --poll --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css",
    "test": "npm run build && mocha --exit"
  },
  "dependencies": {
    "async": "^3.2.4",
    "chai": "^4.3.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "fs-extra": "^10.1.0",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.0.0",
    "mongoose": "^6.5.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.8",
    "redis": "^4.3.0",
    "sharp": "^0.30.7",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "sass": "^1.51.0"
  }
}
