{
  "name": "express-js-fullstack-template",
  "version": "3.2.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "build": "npm run build:scss",
    "build:scss": "sass --style=compressed --error-css --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css",
    "dev": "SET DEBUG=express-js-fullstack-template:* & nodemon",
    "dev:scss:watch": "npm run build:scss && sass --style=compressed --error-css --update --watch --poll --stop-on-error --color --trace ./public/scss/:./public/dist/scss-css"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "fs-extra": "^10.0.1",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.45",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "sass": "^1.49.9"
  }
}