{
  "name": "express-js-fullstack-template",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "SET DEBUG=express-js-fullstack-template:* & nodemon ./bin/www",
    "build": "npm run build:css",
    "build:css": "node-sass --recursive -o ./public/dist/css ./public/scss/",
    "build:css:watch": "npm run build:css && node-sass --recursive -wo ./public/dist/css ./public/scss/"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "rotating-file-stream": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}